# Base network RPC URL
BASE_RPC_URL=https://mainnet.base.org

# Private key for transaction signing (without 0x prefix)
PRIVATE_KEY=your_private_key_here

# Minimum USD value threshold for claiming rewards
MIN_USD_VALUE_THRESHOLD=1.0

# Cron schedule for running the compounder job (default: every 30 minutes)
CRON_SCHEDULE=*/30 * * * *

# Port for the web server
PORT=3000